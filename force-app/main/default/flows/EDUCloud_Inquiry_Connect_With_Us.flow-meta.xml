    <formulas>
        <description>Determines if the user is authenticated based on having a ContactId. This assumes an Experience Site users, so update this formula if the flow should run for internal users.</description>
        <name>IsAuthenticatedUserFormula</name>
        <dataType>Boolean</dataType>
        <expression>IF(NOT(ISBLANK({!$User.ContactId})), true, false)</expression>
    </formulas>
    <formulas>
        <description>Determines the number of Ids in the delimited string by removing the delimiters then dividing by 18 (the size of one Id).</description>
        <name>SelectedLearningProgramsCount</name>
        <dataType>Number</dataType>
        <expression>FLOOR(LEN(SUBSTITUTE({!ProgramsSelect.selectedChoiceValues}, &quot;;&quot;, &quot;&quot;))/18)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>Identifies a single Id substring starting at the indicated left position and counting 18 positions for the length of an Id.</description>
        <name>SelectedLearningProgramSubstring</name>
        <dataType>String</dataType>
        <expression>MID({!ProgramsSelect.selectedChoiceValues}, {!selectedLearningProgramStringLeftPosition}, 18)</expression>
    </formulas>
    <formulas>
        <description>Formula to set the screen&apos;s prepopulated Email to the identified user or null for guest users.</description>
        <name>UserEmailFormula</name>
        <dataType>String</dataType>
        <expression>IF({!IsAuthenticatedUserFormula}, {!$User.Email}, null)</expression>
    </formulas>
    <formulas>
        <description>Formula to set the screen&apos;s prepopulated First Name to the identified user or null for guest users.</description>
        <name>UserFirstNameFormula</name>
        <dataType>String</dataType>
        <expression>IF({!IsAuthenticatedUserFormula}, {!$User.FirstName}, null)</expression>
    </formulas>
    <formulas>
        <description>Formula to set the screen&apos;s prepopulated Last Name to the identified user or null for guest users.</description>
        <name>UserLastNameFormula</name>
        <dataType>String</dataType>
        <expression>IF({!IsAuthenticatedUserFormula}, {!$User.LastName}, null)</expression>
    </formulas>
    <formulas>
        <description>Validates that the First Name is between 2 and 40 characters in length.</description>
        <name>ValidateFirstNameFormula</name>
        <dataType>Boolean</dataType>
        <expression>AND(LEN({!FirstNameText}) &gt;= 2, LEN({!FirstNameText}) &lt;= 40)</expression>
    </formulas>
    <formulas>
        <description>Validate that the Last Name is between 2 and 80 characters in length.</description>
        <name>ValidateLastNameFormula</name>
        <dataType>Boolean</dataType>
        <expression>AND(LEN({!LastNameText}) &gt;= 2, LEN({!LastNameText}) &lt;= 80)</expression>
    </formulas>
    <formulas>
        <description>Validates that the Description is between 4 and 32,000 characters in length.</description>
        <name>ValidationDescriptionFormula</name>
        <dataType>Boolean</dataType>
        <expression>AND(LEN({!DescriptionLongText}) &gt; 4, LEN({!DescriptionLongText}) &lt;= 32000)</expression>
    </formulas>
