    <decisions>
        <description>Decides if there are additional Learning Program Ids to extract based on the current left position and processed Ids count.</description>
        <name>HasNextLearningProgramId</name>
        <label>Has Next Learning Program Id</label>
        <locationX>396</locationX>
        <locationY>458</locationY>
        <defaultConnector>
            <targetReference>IteratePreprocessedSelectedLearningPrograms</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>IsHasNextLearningProgramId</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SelectedLearningProgramsCount</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>extractedLearningProgramIdsCount</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>SelectedLearningProgramsCount</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ExtractLearningProgramId</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
