    <decisions>
        <description>Determines whether an action plan template assignment exists by checking whether the ID is available in the Action Plan Template Assignment from GetActionPlanTemplateAssignment.</description>
        <name>HaveActionPlanTemplateAssignment</name>
        <label>Have Action Plan Template Assignment?</label>
        <locationX>611</locationX>
        <locationY>674</locationY>
        <defaultConnector>
            <targetReference>SendErrorEmailIfMissingActionPlanTemplateAssignment</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No (Default)</defaultConnectorLabel>
        <rules>
            <name>HasActionPlanTemplateAssignment</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>GetActionPlanTemplateAssignment.AssignmentQueueName</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>GetReviewerQueue</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determines whether the required number of application reviews is created.</description>
        <name>HaveCreatedRequiredNumberOfApplicationReviews</name>
