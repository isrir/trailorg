        </defaultConnector>
        <defaultConnectorLabel>No (Default)</defaultConnectorLabel>
        <rules>
            <name>HasCreatedRequiredNumberOfApplicationReviews</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>NumberOfApplicationReviewsCreated</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>GetActionPlanTemplateAssignment.AssignmentSize</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CreateApplicationReviewRecords</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
