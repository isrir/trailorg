        <filterLogic>and</filterLogic>
        <filters>
            <field>ApplicationId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.SubjectId</elementReference>
            </value>
        </filters>
        <filters>
            <field>RecommenderId</field>
